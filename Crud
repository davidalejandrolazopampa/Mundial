import tkinter as tk

window = tk.Tk()
window.title("CRUD")
window.geometry("400x400+300+300")

def insertitem():
    listbox.insert(tk,END,content.get())


label=tk.Label(text ="Insert the items below").pack()
button = tk.Button(text = "Insert Item" , command=insertitem ).pack()
content = tk.StringVar()
entry= tk.Entry(window, textvariable=content).pack()

listbox = tk.Listbox(window).pack()

window.mainloop()
